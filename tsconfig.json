{
  "compilerOptions": {
  "target": "es5",
  "module": "commonjs",
  "outDir": "./dist",
  "rootDir": ".",
  "strict": true,
  "strictPropertyInitialization": false,
  "baseUrl": ".",
  "paths": {
    "@/*":["./src"]
  },
  "allowJs": true,
  "esModuleInterop": true,
  "experimentalDecorators": true,
  "emitDecoratorMetadata": true,
  "forceConsistentCasingInFileNames": true
  },
  "exclude":[
    "jest.config.js",
    "prettier.config.js",
    "node_modules",
    "coverage",
    "tmp"
  ]
}
